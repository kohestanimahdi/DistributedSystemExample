              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ReserveResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReserveResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReserveResponse"
                }
              }
            }
